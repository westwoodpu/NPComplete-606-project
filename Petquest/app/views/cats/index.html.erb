<h1>Listing cats</h1>

<table>
  <tr>
    <th>Name</th>
    <th>Pet</th>
    <th>Type</th>
    <th>Gender</th>
    <th>Age</th>
    <th>Body</th>
    <th>Condition</th>
    <th>Score</th>
    <th>Body</th>
    <th>Weight</th>
    <th>Activity</th>
    <th>Level</th>
    <th>Sterilization</th>
    <th>Time</th>
    <th>Vaccination</th>
    <th>History</th>
    <th>Anthelmintic</th>
    <th>Time</th>
    <th>Vitro</th>
    <th>Flea</th>
    <th>Flooding</th>
    <th>Time</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @cats.each do |cat| %>
  <tr>
    <td><%= cat.name %></td>
    <td><%= cat.Pet %></td>
    <td><%= cat.Type %></td>
    <td><%= cat.Gender %></td>
    <td><%= cat.Age %></td>
    <td><%= cat.Body %></td>
    <td><%= cat.Condition %></td>
    <td><%= cat.Score %></td>
    <td><%= cat.Body %></td>
    <td><%= cat.Weight %></td>
    <td><%= cat.Activity %></td>
    <td><%= cat.Level %></td>
    <td><%= cat.Sterilization %></td>
    <td><%= cat.Time %></td>
    <td><%= cat.Vaccination %></td>
    <td><%= cat.History %></td>
    <td><%= cat.Anthelmintic %></td>
    <td><%= cat.Time %></td>
    <td><%= cat.Vitro %></td>
    <td><%= cat.Flea %></td>
    <td><%= cat.Flooding %></td>
    <td><%= cat.Time %></td>
    <td><%= link_to 'Show', cat %></td>
    <td><%= link_to 'Edit', edit_cat_path(cat) %></td>
    <td><%= link_to 'Destroy', cat, method: :delete, data: { confirm: 'Are you sure?' } %></td>
  </tr>
<% end %>
</table>

