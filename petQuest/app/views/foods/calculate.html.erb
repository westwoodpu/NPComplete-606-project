        <div class="container">
          <!-- Image -->
          <img class="img-responsive" src="http://dakotapethospital.com/clients/14546/images/slideshows/29628/banner%204%20edited.jpg" alt="" />
          <!-- Heading -->
          <h2 class="brown">Nutrient Report</h2>
          <ol class="breadcrumb">
            <li><a href="index.html">Home</a></li>
            <li class="active">Reserve Seats</li>
          </ol>
          <div class="clearfix"></div>
        </div>
      </div>
      <br>
      <!-- Banner End -->
<div class="container">
  <div class ="row">
    <div class="table-responsive col-md-5 col-md-offset-1">
                    <table class="table table-bordered">
                      
                      <tbody>
                        <tr><td><strong><center>Ingredients Composition</center></strong></td></tr>
                        <tr>
                        <tr>
                          <td>Total Weight </td>
                          <td><%='%.2f' % @Totalweight %> g </td>
                        </tr>
                          <td>Meat</td>
                          <td><%='%.2f' % (@Meatproportion + @Poultryproportion + @Fishshellfishproportion )  %> % </td>
                        </tr>
                        <tr>
                          <td>Cereal and Tubers</td>
                          <td><%='%.2f' % ((@Cerealproportion + @Tubersproportion) * @Totalweight) %> % </td>
                        </tr>
                        <tr>
                          <td>Vegetable </td>
                          <td><%='%.2f' % ((@Tubersproportion + @Rootvegetablesproportion + @Leguminousvegetablesproportion + @Solanaceousvegetablesproportion + @Leafyflowervegetablesproportion + @Aquaticproportion) * @Totalweight) %> % </td>
                        </tr>
                        <tr>
                          <td>Oil </td>
                          <td><%='%.2f' % (@Seedfatoilproportion * @Totalweight) %> % </td>
                        </tr>


                        <tr><td><strong><center>Nutrient Analysis</center></strong></td></tr>
                        <tr>
                          <td>Metabolic Energy from Recipe </td>
                          <td><%='%.2f' % @A_ME_result %> KCal/100g </td>
                        </tr>
                        <tr>
                          <td>Estimated Pet's Daily Metabolic Energy </td>
                          <td><%='%.2f' % @me_result %> KCal/100g </td>
                        </tr>
                        <tr>
                          <td>Recommaned Intake for One Meal </td>
                          <td> < <%='%.2f' % (@me_result / 10 * @A_ME_result / 100) %> g </td>
                        </tr>
                        <tr>
                          <td>Protein </td>
                          <td><%= '%.2f' % @Protein %> g/100g</td>
                        </tr>
                        <tr>
                          <td>Fat </td>
                          <td> <%= '%.2f' % @Fat %> g/100g </td>
                        </tr>
                        <tr>
                          <td>Carbohydrate </td>
                          <td> <%='%.2f' % (@CHO-@Dietaryfiber) %> g/100g </td>
                        </tr>                                                                             
                      </tbody>
                    </table></div>
      <div class="table-responsive col-md-5 ">
        <table class="table table-bordered">
                  <tbody>
                      <tr><td><strong><center>Calorie Proportion</center></strong></td></tr>           
                        <tr>
                          <td>Total Calorie </td>
                          <td><%='%.2f' % (@Totalweight * @Energy) %> KCal </td>
                        </tr>
                        <tr>
                          <td>Energy Density </td>
                          <td><%='%.2f' % @Energy %> KCal/100g </td>
                        </tr>
                        <tr>
                          <td>Protein</td>
                          <td><%= '%.2f' %(@Protein*4.0/@Energy*100.00) %> % </td>
                        </tr>
                        <tr>
                          <td>Fat</td>
                          <td><%= '%.2f' %(@Fat*9.0/@Energy*100.0)%> % </td>
                        </tr>
                        <tr>
                          <td>Carbohydrate </td>
                          <td><%='%.2f' % (((@CHO-@Dietaryfiber)*4.0+@Dietaryfiber*2.0)/@Energy*100.0) %> % </td>
                        </tr>



                         <tr><td><strong><center>Nutrition Composition</center></strong></td></tr>                   
                        <tr>
                          <td>Mositure</td>
                          <td><%= '%.2f' % (100-@Protein-@Fat-@CHO-@Ash) %> % </td>
                        </tr>
                        <tr>
                          <td>Protein</td>
                          <td><%= '%.2f' % @Protein %> % </td>
                        </tr>
                        <tr>
                          <td>Fat</td>
                          <td><%= '%.2f' % @Fat %> % </td>
                        </tr>
                        <tr>
                          <td>Carbohydrate</td>
                          <td><%= '%.2f' % (@CHO - @Dietaryfiber) %> % </td>
                        </tr>
                        <tr>
                          <td>Dietary Fiber </td>
                          <td><%='%.2f' % @Dietaryfiber %> % </td>
                        </tr>
                        <tr>
                          <td>Ash </td>
                          <td><%='%.2f' % @Ash %> % </td>
                        </tr>                         
                        
                      </tbody></table></div></div></div>

<br><br>
                <div class="table-responsive col-md-6 col-md-offset-3" align="center">
                    <table class="table table-bordered">
                      
                      <tbody>
                        <tr><td><strong><center>Nutrients Comparison with Recommended</center></strong></td></tr>
                        <tr>
                        <tr>
                          <td>Actual / Recommend Vitamin B1 </td>
                          <td><%='%.2f' % @Vitamin_B1 %> mg / <%='%.2f' % @A_ME_vitamin_b1 %> mg </td>
                        </tr>
                        <tr>
                          <td>Actual / Recommend Vitamin E </td>
                          <td><%='%.2f' % @Vitamin_E %> mg / <%='%.2f' % @A_ME_vitamin_e %> mg </td>
                        </tr>
                        <tr>
                          <td>Actual / Recommend Calcium </td>
                          <td><%='%.2f' % @Calcium %> mg / <%='%.2f' % @A_ME_calcium%> mg </td>
                        </tr>
                        <tr>
                          <td>Actual / Recommend Phosphorous </td>
                          <td><%='%.2f' % @Phosphorous %> mg / <%='%.2f' % @A_ME_phosphorous %> mg </td>
                        </tr>
                        <tr>
                          <td>Actual / Recommend Potassium </td>
                          <td><%='%.2f' % @Potassium %> mg / <%='%.2f' % @A_ME_potassium %> mg </td>
                        </tr>
                        <tr>
                          <td>Actual / Recommend Sodium </td>
                          <td><%='%.2f' % @Sodium %> mg / <%='%.2f' % @A_ME_sodium %> mg </td>
                        </tr>
                        <tr>
                          <td>Actual / Recommend Magnesium </td>
                          <td><%='%.2f' % @Magnesium %> mg / <%='%.2f' % @A_ME_magnesium %> mg </td>
                        </tr>
                        
                         </tbody></table></div></div></div>

        
        
