<h1>nutritionFact list here:</h1>
<%= link_to 'Add nutritionfacts', new_nutritionfact_path %>
<table>
  <tr>

      <th>foodname</th>
      <th>weight</th>
      <th>edible</th>
      <th>energy</th>
      <th>nprotein</th>
      <th>fat</th>
      <th>cho</th>
      <th>dietaryfiber</th>
      <th>ash</th>

    <th colspan="1"></th>
  </tr>
 
  <% @nutritionfacts.each do |nutritionfact| %>
    <tr>

      <td><%= nutritionfact.foodname %></td>
      <td><%= nutritionfact.weight %></td>
      <td><%= nutritionfact.edible %></td>
      <td><%= nutritionfact.energy %></td>
      <td><%= nutritionfact.protein %></td>
      <td><%= nutritionfact.fat %></td>
      <td><%= nutritionfact.cho %></td>
      <td><%= nutritionfact.dietaryfiber %></td>
      <td><%= nutritionfact.ash %></td>

      <td><%= link_to 'Add into Recipe', recipes_path%></td>
    </tr>
  <% end %>
</table>