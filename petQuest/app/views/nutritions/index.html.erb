<p id="notice"><%= notice %></p>

<h1>Listing Nutritions</h1>

<div class="table-responsive">
    <table class="table">
      <thead>
      <tr>
        <th>Category</th>
        <th>Foodname</th>
        <th>Weight(g)</th>
        <th>Edible(g)</th>
        <th>Energy(g)</th>
        <th>Protein(g)</th>
        <th>Fat(g)</th>
        <th>Cho(g)</th>
        <th>Dietaryfiber(g)</th>
        <th>Ash(g)</th>
        <th>Cholesterol(mg)</th>
        <th>VitaminA(ugRE)</th>
        <th>Retinol(ug)</th>
        <th>VitaminE(mg)</th>
        <th>VitaminB1(mg)</th>
        <th>VitaminB2(mg)</th>
        <th>VitaminB3(mg)</th>
        <th>Calcium(mg)</th>
        <th>Phosphorous(mg)</th>
        <th>Potassium(mg)</th>
        <th>Sodium(mg)</th>
        <th>Magnesium(mg)</th>
        <th>Iron(mg)</th>
        <th>Zinc(mg)</th>
        <th>Selenium(ug)</th>
        <th>Copper(mg)</th>
        <th>Manganese(mg)</th>
        <th>Food_id</th>
        <th>VitaminC(mg)</th>
        <th colspan="3"></th>
      </tr>
      </thead>

      <tbody>
        <% @nutritions.each do |nutrition| %>
          <tr>
            <td><%= nutrition.category %></td>
            <td><%= nutrition.foodname %></td>
            <td><%= nutrition.weight %></td>
            <td><%= nutrition.edible %></td>
            <td><%= nutrition.energy %></td>
            <td><%= nutrition.protein %></td>
            <td><%= nutrition.fat %></td>
            <td><%= nutrition.cho %></td>
            <td><%= nutrition.dietaryfiber %></td>
            <td><%= nutrition.ash %></td>
            <td><%= nutrition.cholesterol %></td>
            <td><%= nutrition.vitaminA %></td>
            <td><%= nutrition.retinol %></td>
            <td><%= nutrition.vitaminE %></td>
            <td><%= nutrition.vitaminB1 %></td>
            <td><%= nutrition.vitaminB2 %></td>
            <td><%= nutrition.vitaminB3 %></td>
            <td><%= nutrition.calcium %></td>
            <td><%= nutrition.phosphorous %></td>
            <td><%= nutrition.potassium %></td>
            <td><%= nutrition.sodium %></td>
            <td><%= nutrition.magnesium %></td>
            <td><%= nutrition.iron %></td>
            <td><%= nutrition.zinc %></td>
            <td><%= nutrition.selenium %></td>
            <td><%= nutrition.copper %></td>
            <td><%= nutrition.manganese %></td>
            <td><%= nutrition.food_id %></td>
            <td><%= nutrition.vitaminC%></td>
            <td><%= link_to 'Show', nutrition %></td>
            <td><%= link_to 'Edit', edit_nutrition_path(nutrition) %></td>
            <td><%= link_to 'Destroy', nutrition, method: :delete, data: { confirm: 'Are you sure?' } %></td>
          </tr>
        <% end %>
      </tbody>
    </table>

    <br />
    <%= will_paginate @nutritions %>
    <%= page_entries_info @nutritions, :entry_name => 'nutrition'%> <br />


</div>
<br>

<h3>Import Nutritions</h3>
<%= form_tag import_nutritions_path, multipart: true do %>
  <%= file_field_tag :file %>
  <%= submit_tag "Upload nutritions" %>
<% end %>

<%= link_to 'New Nutrition', new_nutrition_path %>
