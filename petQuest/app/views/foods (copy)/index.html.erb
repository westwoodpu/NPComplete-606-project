<h1>Food list here:</h1>
<%= link_to 'New food', new_food_path %>
<div class="media">
  <div class= "media-body">
    <table class= "table table-condensed">
      <tr>
        <th>picture</th>
        <th>foodCategory</th>
        <th>name</th>
        <th colspan="3"></th>
      </tr>
     
      <% @foods.each do |food| %>
        <tr>
          <td> <img src="/assets/u88.jpg" class="img-responsive" alt="Salmon"> </td>
          <td><%= food.foodCategory %></td>
          <td><%= food.name %></td>
          <td><%= link_to 'Details', food_path(food), class: 'btn btn-mini btn-primary' %></td>
          <td><%= link_to 'NutritionFacts', nutritionfacts_path, class: 'btn btn-mini btn-success' %></td>
          <td><%= link_to 'AddToRecipe', recipes_path, class: 'btn btn-mini btn-danger' %></td>
        </tr>
      <% end %>
    </table>
  </div>
</div>